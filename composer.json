{
    "name": "ipulmisaja/macoa",
    "description": "Plugin sederhana untuk Laravel Framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Syaifur Rijal",
            "email": "syaifur.rijal@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ipulmisaja\\Macoa\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ipulmisaja\\Macoa\\Providers\\AuthenticationServiceProvider",
                "Ipulmisaja\\Macoa\\Providers\\DateFormatServiceProvider",
                "Ipulmisaja\\Macoa\\Providers\\PaginationServiceProvider",
                "Ipulmisaja\\Macoa\\Providers\\FileSizeServiceProvider"
            ],
            "aliases": {
                "DateFormat": "Ipulmisaja\\Macoa\\Helpers\\DateFormat",
                "Pagination": "Ipulmisaja\\Macoa\\Helpers\\Pagination",
                "FileSize": "Ipulmisaja\\Macoa\\Helpers\\FileSize"
            }
        }
    }
}
